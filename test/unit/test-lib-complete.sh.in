#!@SH_PROG@
# -*- shell-script -*-
PS4='(${.sh.file}:${LINENO}): ${.sh.fun} - [${.sh.subshell}] 
'

# Test command completion
test_lib_complete()
{
    typeset -a COMPREPLY
    compgen_opt_words "aa aab ac b a" "aa"
    list="${COMPREPLY[@]}"
    assertEquals 'aa aab' "$list"
    compgen_opt_words "d dd e f" "d"
    list="${COMPREPLY[@]}"
    assertEquals 'd dd' "$list"
}

if [ '@abs_top_srcdir@' = '' ] ; then
  echo "Something is wrong; 'abs_top_srcdir' is not set."
 exit 1
fi
abs_top_srcdir=@abs_top_srcdir@
# Make sure @abs_top_srcr@ has a trailing slash
abs_top_srcdir=${abs_top_srcdir%%/}/
. ${abs_top_srcdir}lib/complete.sh

# load shunit2
shunit_file=${abs_top_srcdir}test/unit/shunit2
. ${shunit_file}
