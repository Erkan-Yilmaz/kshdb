#!@SH_PROG@
# -*- shell-script -*-
PS4='(${.sh.file}:${LINENO}): ${.sh.fun} - [${.sh.subshell}] 
'

# Test _Dbg_glob_filename
test_readin()
{
    typeset filename
    _Dbg_readin ${srcdir}shunit2
    assertEquals '0' $?
    typeset file2
    file2=$(_Dbg_is_file ${srcdir}shunit2)
    assertEquals '0' $?
    assertNotEquals '' "$file2"
}


# Make sure ../.. has a trailing slash
if [ '@abs_top_srcdir@' = '' ] ; then
  echo "Something is wrong abs_top_srcdir is not set."
 exit 1
fi

abs_top_srcdir=@abs_top_srcdir@
abs_top_srcdir=${abs_top_srcdir%%/}/

_Dbg_libdir=$abs_top_srcdir
set -- -q  # Don't need to show banner
. $abs_top_srcdir/dbg-pre.sh
. $abs_top_srcdir/lib/filecache.sh
. $abs_top_srcdir/lib/file.sh

# load shunit2
srcdir=@srcdir@
srcdir=${srcdir%%/}/
. ${srcdir}shunit2

