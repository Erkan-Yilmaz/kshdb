#!@SH_PROG@
# -*- shell-script -*-

test_undefined()
{
    typeset msg=$(_Dbg_undefined_cmd foo bar)
    assertEquals 'Undefined foo command "bar"' "$msg"
}

no__test_progress_show()
{
    typeset EMACS=t
    typeset bar=$(_Dbg_progess_show foo 10 3)
    assertEquals 'foo: [============> ' "${bar:3:20}"
}

top_srcdir=@top_srcdir@
. ${top_srcdir}/lib/msg.sh
. ${top_srcdir}/lib/fns.sh
. ${top_srcdir}/dbg-io.sh

# load shunit2
srcdir=@srcdir@
srcdir=${srcdir}/
. ${srcdir}/shunit2

